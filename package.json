{
  "name": "manga-scraper",
  "version": "1.0.0",
  "description": "A Puppeteer-based manga scraper with bookmark management",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "web": "node src/server.js",
    "dev": "concurrently -n \"backend,frontend\" -c \"cyan,magenta\" \"npm run web\" \"npm run frontend\"",
    "frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "scrape": "node src/index.js scrape",
    "check": "node src/index.js check",
    "list": "node src/index.js list"
  },
  "keywords": [
    "manga",
    "scraper",
    "puppeteer",
    "comix"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "adm-zip": "^0.5.16",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.5.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "ora": "^8.0.1",
    "puppeteer": "^24.31.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sharp": "^0.34.5",
    "socket.io": "^4.8.3",
    "winston": "^3.19.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}